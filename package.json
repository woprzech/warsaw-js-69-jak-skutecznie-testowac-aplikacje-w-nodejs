{
  "name": "warsawjs-use-ur-ide",
  "version": "1.0.0",
  "dependencies": {},
  "devDependencies": {
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/node": "^13.13.5",
    "@types/supertest": "^2.0.9",
    "@types/mongodb": "^3.5.16",
    "@types/mongoose": "^5.7.18",
    "@types/express": "4.17.6",
    "@types/jest": "^24.0.15",
    "@types/axios": "^0.14.0",
    "@types/nock": "^11.1.0",
    "mongodb-memory-server": "^6.6.0",
    "jest": "^24.8.0",
    "axios": "^0.19.2",
    "mongoose": "^5.9.13",
    "express": "^4.17.1",
    "nock": "^10.0.6",
    "ts-jest": "25.5.1",
    "supertest": "4.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "testEnvironment": "node",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/examples/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/examples"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build",
      "<rootDir>/dist"
    ],
    "verbose": true
  }
}
